<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="autofac" type="Autofac.Configuration.SectionHandler, Autofac.Configuration"/>
  </configSections>

  <autofac>
    <components>
      <component
          type="AutofacTest.Services.Implementation.ConsoleOutput, AutofacTest.Services.Implementation"
          service="AutofacTest.Services.Contracts.IOutput, AutofacTest.Services.Contracts" />
      <component
          type="AutofacTest.Services.Implementation.TodayWriter, AutofacTest.Services.Implementation"
          service="AutofacTest.Services.Contracts.IDateWriter, AutofacTest.Services.Contracts" />
    </components>
  </autofac>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>